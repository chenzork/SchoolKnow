<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="10dp">
    
    <TextView
        android:id="@+id/item_getuserinfo_name" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#6192C8"
        android:textSize="16sp"/>
    
    <TextView
        android:id="@+id/item_getuserinfo_stuid" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#787F88"/>
    
    <TextView
        android:id="@+id/item_getuserinfo_collegename" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#787F88"/>
    
    <TextView
        android:id="@+id/item_getuserinfo_classname" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#787F88"/>
    
</LinearLayout>
